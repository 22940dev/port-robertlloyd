{
  "syndicate-to": [{
    "uid": "https://twitter.com/paulrobertlloyd/",
    "name": "@paulrobertlloyd on Twitter"
  }, {
    "uid": "https://mastodon.social/@paulrobertlloyd",
    "name": "@paulrobertlloyd on Mastodon"
  }, {
    "uid": "https://micro.blog/paulrobertlloyd",
    "name": "@paulrobertlloyd on Micro.blog"
  }],
  "post-types": {
    "bookmark": {
      "path": {
        "template": "etc/indiekit/bookmark.njk",
        "post": "src/_content/bookmarks/{{ published | date('yyyy-MM-dd') }}-{{ slug }}.md",
        "url": "bookmarks/{{ published | date('yyyy/MM') }}/{{ slug }}"
      }
    },
    "note": {
      "path": {
        "template": "etc/indiekit/note.njk",
        "post": "src/_content/notes/{{ published | date('X') }}.md",
        "url": "notes/{{ published | date('X') }}"
      }
    },
    "photo": {
      "path": {
        "template": "etc/indiekit/photo.njk",
        "file": "src/images/photos/{{ filedate | date('X') }}.{{ fileext }}",
        "post": "src/_content/photos/{{ published | date('X') }}.md",
        "url": "photos/{{ published | date('X') }}"
      }
    }
  },
  "slug-separator": "_"
}
