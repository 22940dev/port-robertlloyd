{%- layout 'default' -%}
{%- block 'main' -%}
  {%- render 'footer'
    date: date
    type: type
    location: location
    geo: geo
    inReplyTo: inReplyTo
    canonical: canonical
    vocab: vocab
  -%}
  {%- render 'content' with content
    properties: 'e-content p-name'
  -%}
  {%- render 'meta'
    categories: category
    syndication: syndication
  -%}
  {%- render 'remarks' with webmentions
    id: 'webmentions'
    title: 'Webmentions'
  -%}
{%- endblock -%}
