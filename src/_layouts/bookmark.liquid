{%- layout 'default' -%}
{%- block 'main' -%}
  {%- render 'header'
    title: title
    url: bookmark-of
    url_class: 'bookmark-of'
  -%}
  {%- render 'content' with content -%}
  {%- if category.size > 0 -%}
    {%- render 'categories' with category -%}
  {%- endif -%}
  {%- render 'footer'
    date: date
  -%}
{%- endblock -%}
