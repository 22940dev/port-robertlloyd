{%- layout 'default' -%}
{%- block 'main' -%}
  {%- render 'header'
    title: title
    summary: summary
  -%}
  {%- assign items = collections.project | includes: 'data.category',title -%}
  {%- render 'items' with items
    component: 'card'
    grid_size: '12rem'
    item_image_ratio: 1.0
    item_image_preset: 'card'
  -%}
{%- endblock -%}
